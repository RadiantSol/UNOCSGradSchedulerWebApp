<!DOCTYPE html>
<!--TODO: MAKE THIS PRETTY :)-->
<html>
    <head>
        <title>Courses Viewer</title>
    </head>
    <body>
        <table id="table" >  
            <thead>  
                <tr> 
                    <th>ID</th> 
                    <th>Class</th>  
                    <th>Semester</th>  
                    <th>Year</th>
                    <th>Time</th>
                    <th>Credit Hours</th>
                </tr>  
            </thead>  
             <tbody>  
             <% classes.forEach(function (class) { %>
                <tr>  
                    <td><%= class.id %></td>  
                    <td><%= class.classname %></td>
                    <td><%= class.semester %></td>
                    <td><%= class.schoolyear %></td>
                    <td><%= class.startTime %></td>
                    <td><%= class.creditHours %></td>
                </tr>                                   
             <% }); %>
             </tbody>
        </table>   
    </body>
</html>